"*":
  "atom-beautify":
    general:
      _analyticsUserId: "77febff3-70ae-421f-aef4-e848b7ebef9b"
  "atom-package-deps":
    ignored: []
  "autocomplete-clang": {}
  "autocomplete-python":
    pythonPaths: "C:\\Python\\Python36\\python.exe"
    showTooltips: true
    useKite: false
    useSnippets: "required"
  core:
    closeDeletedFileTabs: true
    disabledPackages: [
      "autocomplete-html"
      "django-atom"
      "linter-pylint"
      "linter-rst"
      "linter-clang"
      "linter-gcc"
      "jshint"
      "linter-python-pep257"
      "autocomplete-css"
      "linter-php"
      "linter-phpcs"
      "autocomplete-clang"
      "minimap-autohide"
    ]
    packagesWithKeymapsDisabled: [
      "python-isort"
    ]
    telemetryConsent: "limited"
  editor:
    invisibles: {}
    showIndentGuide: true
    showInvisibles: true
    tabLength: 4
  emmet: {}
  "exception-reporting":
    userId: "8535ec67-e2e7-ea4b-6ca6-53c91442d769"
  "git-plus":
    general:
      _analyticsUserId: "bf118733-17bf-418b-83fe-9ef5cf20deed"
  linter:
    lintOnChange: false
  "linter-clang": {}
  "linter-flake8": {}
  "linter-pep8": {}
  "linter-php": {}
  "linter-phpcs":
    codeStandardOrConfigFile: "PSR2"
    executablePath: "C:\\xampp\\php\\phpcs.bat"
    standard: "PSR2"
    tabWidth: 4
  "linter-pycodestyle":
    convertAllErrorsToWarnings: false
  "linter-pydocstyle":
    ignoreCodes: "D105"
  "linter-ui-default":
    panelHeight: 69
  minimap:
    absoluteMode: true
    plugins:
      cursorline: true
      cursorlineDecorationsZIndex: 0
      "find-and-replace": true
      "find-and-replaceDecorationsZIndex": 0
      "git-diff": true
      "git-diffDecorationsZIndex": 0
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
      linter: true
      linterDecorationsZIndex: 0
      "minimap-autohide": true
      "minimap-autohideDecorationsZIndex": 0
      "minimap-autohider": true
      "minimap-autohiderDecorationsZIndex": 0
      pigments: true
      pigmentsDecorationsZIndex: 0
      selection: true
      selectionDecorationsZIndex: 0
  "python-isort":
    sortOnSave: true
  "rst-preview-pandoc":
    pandocOpts: "-frst -thtml --webtex"
  "spell-check":
    locales: [
      "en-GB"
    ]
  "split-diff":
    diffWords: true
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    scrollSyncType: "Vertical + Horizontal"
  welcome:
    showOnStartup: false
".django.html.text":
  editor:
    tabLength: 4
